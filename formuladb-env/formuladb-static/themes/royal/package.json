{
  "name": "royal-master",
  "version": "1.0.0",
  "description": "Thanks for downloading this theme!",
  "main": "index.html",
  "scripts": {
    "sass": 
    "sass . scss/royal-master-1.scss:css/royal-master-1.css scss/royal-master-2.scss:css/royal-master-2.css scss/royal-master-3.scss:css/royal-master-3.css scss/royal-master-4.scss:css/royal-master-4.css",
    "serve": "live-server --watch=\"*.html\",css",
    "start": "concurrently --kill-others \"npm run sass -- --watch\" \"npm run serve\"",
    "build": "npm run sass -- --style compressed",
