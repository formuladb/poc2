{
  "name": "typescript-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "e2e_serve": "rm -rf dist && tsc --inlineSourceMap -p src && concurrently --kill-others \"./node_modules/.bin/pouchdb-server --in-memory --port 5984\" \"sleep 15 && node dist/server/src/server.js\"",
    "preserve": "rm -rf dist && tsc --inlineSourceMap -p src",
    "inspect": "kill-port 9229 && node --inspect=0.0.0.0:9229 dist/server/src/server.js",
    "nodemon-inspect": "nodemon --delay 100ms --watch dist --exec \"npm run inspect\"",
    "serve": "concurrently --kill-others \"pouchdb-server --in-memory --port 5984\" \"tsc --inlineSourceMap -w -p src\" \"wait-port -t 20000 localhost:5984 && sleep 5 && npm run nodemon-inspect\"",
    "prestart": "rm -rf dist && tsc --inlineSourceMap -p src",
    "start": "node dist/server/src/server.js",
    "test": "./node_modules/.bin/mocha \"dist/server/**/*.spec.js\""
  },
  "dependencies": {
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.102",
    "@types/node": "^8.0.9",
    "@types/passport": "^0.3.3",
    "body-parser": "~1.13.2",
    "concurrently": "^3.5.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.15.6",
    "expression-eval": "^1.2.0",
    "glob": "^5.0.14",
    "jsep": "^0.3.3",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "morgan": "^1.6.1",
    "pouchdb": "^6.3.4",
    "pouchdb-find": "^6.3.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "serve-favicon": "~2.3.0",
    "source-map-support": "^0.5.3",
    "typedi": "^0.6.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/chai-subset": "^1.3.1",
    "@types/mocha": "^2.2.46",
    "chai": "^4.1.2",
    "chai-subset": "^1.6.0",
    "cross-port-killer": "^1.0.1",
    "memdown": "^2.0.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.14.12",
    "pouchdb-server": "^4.0.0",
    "rxjs-marbles": "^2.3.0",
    "rxjs-test": "^5.5.0-beta.0",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.16",
    "typescript": "^2.4.1"
  }
}
