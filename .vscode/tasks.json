{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "_runner": "terminal",
    "tasks": [
        {
            "label": "serve:maps",
            "command": "npm run serve:maps",
            "type": "shell",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn":["docker:watch"]
        },
        {
            "label": "docker:watch",
            "command": "npm run docker:watch",
            "type": "shell",
            "group": "build",
            "dependsOn":["bundle:watch"]
        },
        {
            "label": "bundle:watch",
            "command": "npm run bundle:watch",
            "type": "shell",
            "group": "build",
            "dependsOn":["compile:watch"]
        },
        {
            "label": "compile:watch",
            "command": "npm run compile:watch",
            "type": "shell",
            "group": "build",
            "problemMatcher": "$tsc",
        },
        {
            "label": "BE serve",
            "command": "npm",
            "type": "shell",
            "options": {
                "cwd": "./be"
            },
            "args": [
                "run",
                "serve"
            ],
            "terminalGroup": "run_febe",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared"
            },
            "isBackground": true,
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        }
    ]
}