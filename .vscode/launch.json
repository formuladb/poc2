{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "jasmine",
            "type": "node",
            "useWSL": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/d/code/metawiz/febe",
            "runtimeArgs": [
                "--trace-warnings",
                "--debug-brk",
                "--inspect=0.0.0.0:9232"
            ],
            "env": {
                "DEBUG": "formuladb:*"
            },
            "protocol": "inspector",
            "port": 9232,
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}/jasmine.js",
            "args": [
                "'${relativeFile}'"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/tsc-out/**/*.js",
            ]
        },
        {
            "name": "FE serve",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8081",
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "webpack:///./domain/src/*": "${webRoot}/domain/src/*",
                "webpack:///./functions/src/*": "${webRoot}/functions/src/*",
                "webpack:///./core/src/*": "${webRoot}/core/src/*",
                "webpack:///./test/src/*": "${webRoot}/test/src/*",
                "webpack:///./fe/src/*": "${webRoot}/fe/src/*",
            }
        },
        {
            "name": "BE serve",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "protocol": "auto",
            "restart": true,
            "port": 9231,
            "localRoot": "${workspaceFolder}/dist-be",
            "remoteRoot": "/dist-be",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist-be/*.js"
            ],
            "skipFiles": [
                "<eval>/**/*.js",
                "<node_internals>/**/*.js",
            ],
            "trace": true
        },
        {
            "name": "BE specs mem",
            "type": "node",
            "useWSL": true,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/d/code/metawiz/febe",
            "runtimeArgs": [
                "--trace-warnings",
                "--debug-brk",
                "--inspect=0.0.0.0:9232"
            ],
            "env": {},
            "protocol": "inspector",
            "port": 9232,
            "request": "launch",
            "cwd": "${workspaceRoot}/be",
            "program": "${workspaceRoot}/be/src/tests/jasmine.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/be/dist/**/*.js"
            ]
        },
        {
            "name": "BE specs postgres",
            "type": "node",
            "useWSL": true,
            "runtimeArgs": [
                "--trace-warnings",
                "--debug-brk",
                "--inspect=0.0.0.0:9232"
            ],
            "env": {
                "FRMDB_STORAGE": "postgres"
            },
            "protocol": "inspector",
            "port": 9232,
            "request": "launch",
            "cwd": "${workspaceRoot}/be",
            "program": "${workspaceRoot}/be/src/tests/jasmine.ts",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/be/dist/**/*.js"
            ]
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "FE specs",
            "address": "localhost",
            "port": 9369,
            "sourceMaps": true,
            "restart": "true",
            "pathMapping": {
                "/": "${workspaceRoot}/febe",
                "/base/": "${workspaceRoot}/febe"
            }
        },
        {
            "name": "ng e2e",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/fe/node_modules/protractor/bin/protractor",
            "protocol": "inspector",
            "args": [
                "${workspaceFolder}/fe/e2e/protractor.conf.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "node-current-file",
            "protocol": "inspector",
            "args": [
                "${relativeFile}"
            ],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "protocol": "inspector",
            "request": "attach",
            "name": "Attach to Remote BE",
            "address": "127.0.0.1",
            "port": 9229,
            "trace": "sm",
            "sourceMaps": true,
            //            "restart": true,
            //            "outFiles": ["${workspaceRoot}/be/dist/be/src/"],
            //            "localRoot": "${workspaceRoot}/be/dist/be/src/",
            //            "remoteRoot": "/dist/be/src",
            //            "sourceMapPathOverrides": {
            //                "webpack:///*": "${workspaceRoot}/be/*"
            //            }
        }
    ]
}