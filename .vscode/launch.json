{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ng serve",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4200/#",
            "webRoot": "${workspaceRoot}/febe"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to running Node",
            "port": 9229
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${file}",
            "outFiles": [
                "${workspaceRoot}/out/**/*.js"
            ]
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach Karma Chrome",
            "address": "localhost",
            "port": 9222,
            "sourceMaps": true,
            "pathMapping": {
                "/": "${workspaceRoot}/febe",
                "/base/": "${workspaceRoot}/febe"
            }
        }
    ]
}
