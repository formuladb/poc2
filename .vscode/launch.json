{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "jasmine",
            "type": "node",
            "runtimeArgs": [
                "--trace-warnings",
                "--debug-brk",
                "--inspect=0.0.0.0:9232"
            ],
            "env": {
                "DEBUG": "formuladb:*"
            },
            "protocol": "inspector",
            "port": 9232,
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}/jasmine.js",
            "args": [
                "${relativeFile}"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/tsc-out/**/*.js",
            ]
        },
        {
            "name": "jasmine postgres",
            "type": "node",
            "runtimeArgs": [
                "--trace-warnings",
                "--debug-brk",
                "--inspect=0.0.0.0:9232"
            ],
            "env": {
                "DEBUG": "formuladb:*",
                "FRMDB_STORAGE": "postgres",
                "WSLENV": "FRMDB_STORAGE"
            },
            "protocol": "inspector",
            "port": 9232,
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}/jasmine.js",
            "args": [
                "${relativeFile}"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/tsc-out/**/*.js",
            ]
        },
        {
            "name": "jasmine fe",
            "type": "node",
            "runtimeArgs": [
                "--trace-warnings",
                "--debug-brk",
                "--inspect=0.0.0.0:9232"
            ],
            "env": {
                "DEBUG": "formuladb:*"
            },
            "protocol": "inspector",
            "port": 9232,
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}/jasmine.js",
            "args": [
                "browser"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/tsc-out/**/*.js",
            ]
        },
        {
            "name": "FE serve",
            "type": "chrome",
            "request": "launch",
            "-url": "http://frmdb.localhost/en-basic-008cba-eeeeee-Clean/hotel-booking/booking.html?frmdbRender=editor&%24FRMDB.Room_Booking%5B%5D.start_date=2020-03-23&%24FRMDB.Room_Booking%5B%5D.end_date=2020-03-26&%24FRMDB.Room_Booking%5B%5D.nb_adults=1&%24FRMDB.Room_Booking%5B%5D.nb_children=0",
            "url": "http://frmdb.localhost/en-basic-008cba-eeeeee-Clean/hotel-booking/index.html?frmdbRender=editor",
            "--url": "http://frmdb.localhost/en-basic-cb8670-363636-Clean/base-app/overlays.html",
            "webRoot": "${workspaceFolder}",
            "trace": true,
            "sourceMapPathOverrides": {
                "webpack:///./domain/src/*": "${webRoot}/domain/src/*",
                "webpack:///./functions/src/*": "${webRoot}/functions/src/*",
                "webpack:///./core/src/*": "${webRoot}/core/src/*",
                "webpack:///./test/src/*": "${webRoot}/test/src/*",
                "webpack:///./fe/src/*": "${webRoot}/fe/src/*",
            }
        },
        {
            "name": "BE serve",
            "type": "node",
            "request": "attach",
            "address": "localhost",
            "protocol": "auto",
            "restart": true,
            "port": 9231,
            "localRoot": "${workspaceFolder}/dist-be",
            "remoteRoot": "/dist-be",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/dist-be/*.js"
            ],
            "skipFiles": [
                "<eval>/**/*.js",
                "<node_internals>/**/*.js",
            ],
            "trace": true
        },
        {
            "name": "cli",
            "type": "node",
            "runtimeArgs": [
                "--trace-warnings",
                "--debug-brk",
                "--inspect=0.0.0.0:9232"
            ],
            "env": {
                "DEBUG": "formuladb:*",
                "FRMDB_ENV_ROOT_DIR": "/mnt/d/code/metawiz/febe/git",
            },
            "protocol": "inspector",
            "port": 9232,
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}/dist-be/frmdb-be.js",
            "-args": [
                "init-db"
            ],
            "args": [
                "fix-html",
                "${relativeFile}"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/tsc-out/**/*.js",
            ]
        },
    ],
}