{
  "name": "fe",
  "version": "0.0.0",
  "scripts": {
    "build-high-memory": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build",
    "ng": "ng",
    "dev": "concurrently --kill-others \"ng serve --progress=false\" \"ng test --progress=false\" | perl -pe 's!^\\[\\d+\\] !!; s!ERROR in !!; s!^src/app/!fe/src/app/!'",
    "start": "ng serve --aot --proxy-config proxy.conf.json --source-map",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cross-env TARGET=headless ng e2e --port 4300",
    "e2e-with-gui": "cross-env TARGET=gui ng e2e --port 4300",
    "e2e-with-recordings": "cross-env TARGET=recordings ng e2e --port 4300",
    "e2e-with-recordings-and-audio": "cross-env TARGET=recordings-with-audio ng e2e --port 4300"
  },
