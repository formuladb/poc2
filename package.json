{
  "name": "formuladb",
  "version": "0.0.10",
  "description": "FormulaDB low code platform",
  "main": "index.js",
  "scripts": {
    "start": "rimraf dist && concurrently --kill-others \"npm run sass:watch\" \"npm run build:watch\" \"npm run serve\"",
    "inspect": "kill-port 9231 && concurrently --kill-others \"npm start || true\" \"npm run test-data\" ",
    "serve": "webpack-dev-server",
    "compile:watch": "tsc --sourceMap -w -p .",
    "build:watch": "webpack --progress --watch",
    "sass:watch": "sass --style compressed --watch . live-dom-editor/src/live-dom-editor.scss:dist/live-dom-editor.min.css",
    "build": "webpack",
    "be-spec": "jasmine --helper=./jasmine.helpers.js tsc-out/core/src/frmdb_engine.spec.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
