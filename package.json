{
  "name": "formuladb",
  "version": "0.0.10",
  "description": "FormulaDB low code platform",
  "main": "index.js",
  "scripts": {
    "start:be": "node --inspect=0.0.0.0:9231 dist/be.js",
    "start": "npm run start:be",
    "inspect": "kill-port 9231 && concurrently --kill-others \"npm start || true\" \"npm run test-data\" ",
    "serve": "cross-env FRMDB_STORAGE=postgres concurrently --kill-others \"npm run compile:watch\" \"npm run bundle:watch\" \"npm run serve:be\" ",
    "serve:be": "nodemon --delay 200ms --watch dist --exec \"npm run start:be\"",
    "serve:fe": "live-server --watch=\"dist,data-grid/index.html\" --port=8081 --ignore=\"*/node_modules,*/src\" --no-browser -V",
    "compile:watch": "tsc --sourceMap -w -p .",
    "bundle:watch": "webpack --progress --watch --mode development",
    "sass:watch": "sass --style compressed --watch . live-dom-editor/src/live-dom-editor.scss:dist/live-dom-editor.min.css",
    "build": "webpack",
    "be-spec": "jasmine --helper=./jasmine.helpers.js tsc-out/core/src/frmdb_engine.spec.js",
    "test": "node --inspect=0.0.0.0:9232 jasmine.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@google-cloud/storage": "^2.5.0",
    "@types/jasmine": "^3.3.12",
    "@types/jquery": "^3.3.29",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.0.2",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "dom-testing-library": "^4.1.0",
    "glob": "^7.1.4",
    "install": "^0.12.2",
    "jasmine": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jsdom": "^15.1.0",
    "jsdom-global": "^3.0.2",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm": "^6.9.0",
    "parallel-webpack": "^2.3.0",
    "raw-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass": "^1.20.1",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.2.4",
    "testem": "^2.16.0",
    "ts-loader": "^6.0.1",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "3.1.6",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
