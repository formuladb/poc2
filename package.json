        "jsdom-devtools-formatter": "^1.0.0",
        "jsdom-global": "^3.0.2",
        "line-awesome": "^1.3.0",
        "live-server": "^1.2.1",
        "lolex": "^5.1.2",
        "mp3-duration": "^1.1.0",
        "node-sass": "^4.13.0",
        "npm": "^6.13.6",
        "parallel-webpack": "^2.4.0",
        "protractor": "5.4.2",
        "raw-loader": "^4.0.0",
        "rimraf": "^3.0.0",
        "sass": "^1.24.4",
        "sass-loader": "^8.0.2",
        "shelljs": "^0.8.3",
        "source-map-loader": "^0.2.4",
        "terser-webpack-plugin": "^2.3.2",
        "testem": "^3.0.2",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "ts-node-dev": "^1.0.0-pre.41",
        "tsconfig-paths": "^3.9.0",
        "typescript": "3.7.4",
        "typescript-json-schema": "^0.42.0",
        "watch": "^1.0.2",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-node-externals": "^1.7.2",
