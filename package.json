    "cross-env": "^5.2.0",
    "csv": "^5.1.1",
    "debug": "~2.2.0",
    "delegated-events": "^1.0.10",
    "detect-node": "^2.0.4",
    "dotenv": "^6.2.0",
    "express": "~4.13.1",
    "express-session": "^1.15.6",
    "expression-eval": "git+https://github.com/acristu/expression-eval.git",
    "formulajs": "git+https://github.com/acristu/formula.js.git",
    "fuse.js": "^3.3.0",
