{
    "name": "formuladb",
    "version": "0.0.22",
    "description": "FormulaDB low code platform",
    "scripts": {
        "start": "bash scripts/init-be.sh && node --max-old-space-size=4096 --inspect=0.0.0.0:9231 dist-be/frmdb-be.js",
        "start_dev": "bash scripts/init-be.sh && nodemon --max-old-space-size=2048 --watch /dist-be --inspect=0.0.0.0:9231 dist-be/frmdb-be.js",
        "_rem_precompile": "patch --merge node_modules/protractor/built/index.d.ts  e2e/protractor.patch",
        "compile": "tsc --sourceMap -p .",
        "start-dev-fe": "BUILD_DEVELOPMENT=1 FRMDB_ENV_ROOT_DIR=$PWD nodemon --watch dist-be --inspect=0.0.0.0:9231 dist-be/frmdb-be.js",
        "watch-x-compile": "npm run compile -- -w --preserveWatchOutput",
        "bundle": "webpack --mode development",
        "bundle:watch": "concurrently -k \"webpack --watch --mode development\" \"npm run watch-sass\" ",
        "tmux-bundle": "webpack --watch --mode development --progress",
        "watch-x-bundle": "webpack --watch --mode development",
        "docker:be": "docker build -t formuladb-be -f Dockerfile.be . && docker tag formuladb-be registry.gitlab.com/metawiz/febe/formuladb-be:${npm_package_version} && docker tag formuladb-be registry.gitlab.com/metawiz/febe/formuladb-be:latest",
        "docker:fe": "docker build -t formuladb-maps -f Dockerfile.fe . && docker tag formuladb-fe registry.gitlab.com/metawiz/febe/formuladb-fe:${npm_package_version}",
        "docker:maps": "docker build -t formuladb-maps -f Dockerfile.maps . && docker tag formuladb-maps registry.gitlab.com/metawiz/febe/formuladb-maps:${npm_package_version}",
        "_skaffold_": "KUBECONFIG=k8s/production-kube-config.conf skaffold dev -n \"`git branch|grep '^*'|cut -d ' ' -f2`\" -p dev --port-forward=true",
        "skaffold": "skaffold dev -n \"frmdb-`git branch|grep '^*'|cut -d ' ' -f2`\" -p dev --port-forward=true",
        "watch-skaffold": "npm run skaffold",
        "watch-x-sass": "sleep 10; node-sass --watch --output-style=compressed fe/src/frmdb-editor/frmdb-editor.directive.scss formuladb/frmdb-editor.min.css",
        "_rem_watch-rsync": "sleep 15; while true; do sleep 3; bash rsync.sh 2>/dev/null; done",
        "live-server": "concurrently -k \"npm run watch-rsync\" \"bash serve.sh\" ",
        "-rem-watch-live-server": "bash serve.sh",
        "serve-tmux": "bash ci/prepare-dev.sh && tmux set -g status off ';' new-session 'npm run watch-x-compile' ';' split-window -h 'npm run tmux-bundle' ';' split-window -h 'npm run watch-skaffold' ';' select-layout tiled",
        "serve": "bash ci/prepare-dev.sh && concurrently -k \"npm:watch-*\" ",
        "serve-fe": "bash ci/prepare-dev.sh && concurrently -k \"npm:watch-x-*\" \"npm run start-dev-fe\" ",
        "test": "node jasmine.js",
        "test:fe": "node jasmine.js browser",
        "e2e": "protractor e2e/protractor.conf.js",
        "webdriver-update": "webdriver-manager update",
        "dev:be": "ts-node-dev --respawn --transpileOnly ./be/src/server.ts",
        "preprocess-icons": "svgo -f formuladb/icons/svg -o formuladb/icons/svgo --disable=removeViewBox --enable=removeDimensions --config '{\"plugins\": [{\"convertPathData\": {\"noSpaceAfterFlags\": false}}, {\"mergePaths\": {\"noSpaceAfterFlags\": false}}]}'",
        "generate-icons": "icon-font-generator -p frmdb-i formuladb/icons/svgo/*.svg -o formuladb/icons -n frmdb-icons",
        "generate-json-schemas": "for i in App Entity SchemaEntityList; do typescript-json-schema ./tsconfig.json $i > core/src/schemas/$i.schema.json ; done",
        "generate-look-css": "cd themes; bash ci/build.sh --compile",
        "--example--watch-css": "export PATH=$PATH:../node_modules/.bin; watch 'cd themes; bash ci/build.sh --compile' themes",
        "sync-dev-static-assets": "watch 'bash ci/copy-static-assets-to-dev-ngunx-pod.sh' formuladb-env/static formuladb-themes"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.0.20",
        "@google-cloud/text-to-speech": "^2.1.2",
        "@types/bootstrap": "^4.3.1",
        "@types/connect-flash": "0.0.35",
        "@types/d3": "^5.7.2",
        "@types/express": "^4.17.4",
        "@types/express-session": "^1.17.0",
        "@types/jasmine": "^3.5.0",
        "@types/jquery": "^3.3.31",
        "@types/js-yaml": "^3.12.1",
        "@types/lodash": "^4.14.149",
