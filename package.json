{
    "name": "formuladb",
    "version": "0.0.12",
    "description": "FormulaDB low code platform",
    "scripts": {
        "start": "node --inspect=0.0.0.0:9231 dist-be/frmdb-be.js",
        "compile": "tsc --sourceMap -p .",
        "compile:watch": "npm run compile -- -w",
        "bundle": "webpack",
        "bundle:watch": "webpack --watch --progress --mode development",
        "docker:be": "docker build -t formuladb-be -f Dockerfile.be . && docker tag formuladb-be registry.gitlab.com/metawiz/febe/formuladb-be:${npm_package_version} && docker tag formuladb-be registry.gitlab.com/metawiz/febe/formuladb-be:latest",
        "docker:fe": "docker build -t formuladb-maps -f Dockerfile.fe . && docker tag formuladb-fe registry.gitlab.com/metawiz/febe/formuladb-fe:${npm_package_version}",
        "docker:maps": "docker build -t formuladb-maps -f Dockerfile.maps . && docker tag formuladb-maps registry.gitlab.com/metawiz/febe/formuladb-maps:${npm_package_version}",
        "build:watch": "tmux new-session 'npm run compile:watch' ';' split-window -h 'npm run bundle:watch' ';' split-window -h 'bash serve.sh' ';' select-layout even-horizontal",
        "serve": "tmux new-session 'npm run compile:watch' ';' split-window -h 'npm run bundle:watch' ';' split-window -h 'npm run docker:watch' ';' split-window -h 'bash serve.sh' ';' select-layout even-horizontal",
        "test": "node jasmine.js",
        "e2e:data": "FRMDB_STORAGE=postgres node tsc-out/test/src/load_test_data.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@google-cloud/storage": "^2.5.0",
        "@tbranyen/jsdom": "^13.0.0",
        "@types/jasmine": "^3.3.12",
        "@types/jquery": "^3.3.29",
        "@types/js-yaml": "^3.12.1",
        "@types/lodash": "^4.14.135",
        "@types/node": "^12.0.2",
        "browser-sync": "^2.26.7",
        "concurrently": "^4.1.0",
        "document-register-element": "^1.13.2",
        "fetch-mock": "^7.3.6",
        "glob": "^7.1.4",
        "install": "^0.12.2",
        "jasmine": "^3.4.0",
        "jasmine-spec-reporter": "^4.2.1",
        "jsdom": "^15.1.0",
        "jsdom-devtools-formatter": "^1.0.0",
        "jsdom-global": "^3.0.2",
        "live-server": "^1.2.1",
        "lolex": "^4.1.0",
        "node-fetch": "^2.6.0",
        "node-sass": "^4.12.0",
        "nodemon": "^1.19.1",
        "npm": "^6.9.0",
        "parallel-webpack": "^2.3.0",
        "pretty": "^2.0.0",
        "raw-loader": "^3.0.0",
        "rimraf": "^2.6.3",
        "sass": "^1.20.1",
        "sass-loader": "^7.1.0",
        "source-map-loader": "^0.2.4",
        "terser-webpack-plugin": "^1.3.0",
        "testem": "^2.16.0",
        "ts-loader": "^6.0.1",
        "ts-node": "^8.2.0",
        "tsconfig-paths": "^3.8.0",
        "typescript": "3.5.2",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.2",
        "webpack-node-externals": "^1.7.2",
        "whatwg-fetch": "^3.0.0"
    },
    "dependencies": {
        "acorn": "^5.4.1",
        "ag-grid-community": "^21.1.0",
        "ag-grid-enterprise": "^21.1.0",
        "body-parser": "~1.13.2",
        "cluster-key-slot": "^1.0.12",
        "connect-ensure-login": "^0.1.1",
        "cookie-parser": "~1.3.5",
        "cross-env": "^5.2.0",
        "csv": "^5.1.1",
        "debug": "^4.1.1",
        "detect-node": "^2.0.4",
        "dompurify": "^1.0.11",
        "dotenv": "^6.2.0",
        "express": "~4.13.1",
        "express-session": "^1.15.6",
        "expression-eval": "git+https://github.com/acristu/expression-eval.git",
        "formulajs": "git+https://github.com/acristu/formula.js.git",
        "fuse.js": "^3.3.0",
        "http-proxy-middleware": "^0.19.1",
        "js-yaml": "^3.13.1",
        "jsep": "git+https://github.com/acristu/jsep.git",
        "lodash": "^4.17.11",
        "md5": "^2.2.1",
        "module-alias": "^2.1.0",
        "moment": "^2.24.0",
        "morgan": "^1.6.1",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "pg": "^7.8.0",
        "pg-promise": "8.2.1",
        "pouchdb-collate": "^7.0.0",
        "serve-favicon": "~2.3.0",
        "short-uuid": "^3.1.0",
        "source-map-support": "^0.5.12",
        "tslib": "^1.9.3",
        "wait-on": "^3.2.0"
    },
    "_moduleAliases": {
        "@domain": "./tsc-out/domain/src",
        "@functions": "./tsc-out/functions/src",
        "@core": "./tsc-out/core/src",
        "@storage": "./tsc-out/storage/src",
        "@test": "./tsc-out/test/src",
        "@be": "./tsc-out/be/src",
        "@fe": "./tsc-out/fe/src",
        "@fe-assets": "./fe/src",
        "raw-loader!@fe-assets": "./fe/src",
        "!!raw-loader!sass-loader?sourceMap!@fe-assets": "./fe/src",
        "raw-loader!@test-assets": "./test/src"
    }
}
