{
  "name": "formuladb-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "e2e_serve": "rm -rf dist && tsc --sourceMap -p src && concurrently --kill-others \"./node_modules/.bin/pouchdb-server --in-memory --port 5984\" \"sleep 15 && node dist/be/src/server.js\"",
    "preserve": "rm -rf dist && tsc --sourceMap -p src",
    "inspect": "kill-port 9231 && concurrently \"DEBUG=express:* node --inspect=0.0.0.0:9231 dist/be/src/server.js\" \"npm run test-data\" ",
    "nodemon-inspect": "nodemon --delay 200ms --watch dist --exec \"npm run inspect\"",
    "serve": "cross-env FRMDB_STORAGE=postgres concurrently --kill-others \"tsc --sourceMap -w -p src\" \"sleep 10 && npm run nodemon-inspect\"",
    "start": "node --inspect dist/be/src/server.js",
    "compile": "tsc --sourceMap -p src",
    "compile:watch": "tsc --sourceMap -w -p src",
    "pretest": "npm run compile",
    "test": "node dist/be/src/tests/jasmine.js",
    "TODOOO": "filter test|serve|compile|inspect tasks from Dockerfile, just to be on the safe side in production",
    "test-data": "wait-on tcp:3000 && node -e 'require(\"module-alias/register\"); require(\"@core/test/load_test_data.js\").loadTestData()'",
    "exampleapps": "nodemon --delay 200ms --exec \"TBD generate json/csv for test data and metadata\" --watch ../core/src/test/mocks/ --watch ../core/src/domain/metadata/default-metadata.ts --watch ../core/src/domain/metadata/default-data.ts",
    "jasmine:watch": "watch \"node --inspect dist/be/src/tests/jasmine.js\" dist --wait=1 --interval=2",
    "test:watch": "concurrently --kill-others \"npm run compile -- -w\" \"sleep 2 && npm run jasmine:watch\" ",
    "build-prod": "rm -rf dist && webpack"
  },
  "dependencies": {
    "acorn": "^5.4.1",
    "axios": "^0.18.0",
    "body-parser": "~1.13.2",
    "cluster-key-slot": "^1.0.12",
    "concurrently": "^3.5.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "cross-env": "^5.2.0",
    "debug": "~2.2.0",
    "detect-node": "^2.0.4",
    "dotenv": "^6.2.0",
    "express": "~4.13.1",
    "express-session": "^1.15.6",
    "formulajs": "git+https://github.com/acristu/formula.js.git",
    "fuse.js": "^3.3.0",
    "glob": "^5.0.14",
    "http-proxy-middleware": "^0.19.1",
    "jsep": "git+https://github.com/acristu/jsep.git",
    "lodash": "^4.17.5",
    "md5": "^2.2.1",
    "module-alias": "^2.1.0",
    "moment": "^2.24.0",
    "morgan": "^1.6.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "pg-promise": "^8.5.5",
    "pouchdb-collate": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "serve-favicon": "~2.3.0",
    "short-uuid": "^3.0.0",
    "source-map-support": "^0.5.3",
    "tslib": "^1.9.3",
    "typedi": "^0.6.1",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.3",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.8.6",
    "@types/lodash": "^4.14.120",
    "@types/node": "^8.0.9",
    "@types/passport": "^0.3.3",
    "cross-port-killer": "^1.0.1",
    "jasmine": "^3.0.0",
    "jasmine-spec-reporter": "^4.2.1",
    "memdown": "^2.0.0",
    "nodemon": "^1.14.12",
    "rxjs-marbles": "^2.3.0",
    "rxjs-test": "^5.5.0-beta.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.16",
    "typescript": "3.1.6",
    "uuid": "^3.2.1",
    "watch": "^1.0.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "_moduleAliases": {
    "@storage": "./dist/storage/src",
    "@core": "./dist/core/src"
  }
}
