{
  "name": "formuladb-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "e2e_serve": "rm -rf dist && tsc --inlineSourceMap -p src && concurrently --kill-others \"./node_modules/.bin/pouchdb-server --in-memory --port 5984\" \"sleep 15 && node dist/be/src/server.js\"",
    "preserve": "rm -rf dist && tsc --inlineSourceMap -p src",
    "inspect": "kill-port 9229 && node --inspect=0.0.0.0:9229 dist/be/src/server.js",
    "nodemon-inspect": "nodemon --delay 100ms --watch dist --exec \"npm run inspect\"",
    "serve": "concurrently --kill-others \"pouchdb-server --in-memory --port 5984\" \"tsc --inlineSourceMap -w -p src\" \"wait-port -t 20000 localhost:5984 && sleep 5 && npm run nodemon-inspect\"",
    "prestart": "rm -rf dist && tsc --inlineSourceMap -p src",
    "start": "node dist/be/src/server.js",
    "compile": "tsc --inlineSourceMap -p src",
    "compile:watch": "tsc --inlineSourceMap -p src -w 2>&1 | perl -pe 's!^../src/app/common!\\n./src/app/common!; s!^src/(?\\!app/common)!./be/src/!'",
    "pretest": "npm run compile",
    "test": "node dist/be/src/tests/jasmine.js",
    "jasmine:watch": "watch \"node --inspect dist/be/src/tests/jasmine.js\" dist --wait=1 --interval=2",
    "test:watch": "concurrently --kill-others \"npm run compile -- -w\" \"sleep 2 && npm run jasmine:watch\" "
  },
  "dependencies": {
    "acorn": "^5.4.1",
    "axios": "^0.18.0",
    "body-parser": "~1.13.2",
    "concurrently": "^3.5.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-session": "^1.15.6",
    "expression-eval": "git+https://github.com/acristu/expression-eval.git",
    "fuse.js": "^3.3.0",
    "glob": "^5.0.14",
    "morgan": "^1.6.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^5.5.6",
    "serve-favicon": "~2.3.0",
    "short-uuid": "^3.0.0",
    "source-map-support": "^0.5.3",
    "jsep": "git+https://github.com/acristu/jsep.git",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "pg": "^7.8.0",
    "pouchdb-collate": "^7.0.0",
    "typedi": "^0.6.1"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.3",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.8.6",
    "@types/lodash": "^4.14.120",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.9",
    "@types/passport": "^0.3.3",
    "cross-port-killer": "^1.0.1",
    "jasmine": "^3.0.0",
    "jasmine-spec-reporter": "^4.2.1",
    "memdown": "^2.0.0",
    "nodemon": "^1.14.12",
    "rxjs-marbles": "^2.3.0",
    "rxjs-test": "^5.5.0-beta.0",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.16",
    "typescript": "^2.8.3",
    "uuid": "^3.2.1",
    "watch": "^1.0.2"
  }
}
