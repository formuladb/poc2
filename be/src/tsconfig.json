{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "outDir": "../dist",
        "baseUrl": "./",
        "module": "commonjs",
        "strictNullChecks": true,
        "paths": {
            "@core/*": ["../../core/src/*"],
            "@storage/*": ["../../storage/src/*"],
	        "@kv_selector_base/*": ["./*"]
        },
        "sourceMap": true
    },
    "include": [
        "./**/*",
        "../../core/src/**/*",
        "../../storage/src/**/*"
    ]
}
