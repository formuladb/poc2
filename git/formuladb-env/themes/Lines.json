{
    ".frmdb-t-main-nav": {
        "addClasses": [
            "container",
            "bg-white",
            "fixed-top",
            "shadow"
        ]
    },
    ".frmdb-t-main-nav .navbar": {
        "addClasses": [
            "navbar-light",
            "navbar-expand-lg"
        ]
    },
    ".frmdb-t-cover": {
        "addClasses": [
            "vh-120",
            "overflow-hidden",
            "frmdb-bg-3",
            "frmdb-bg-2-dark-solid-tint-50",
            "frmdb-bg-3-divider-slope"
        ],
        "addStyles": {
            "background-repeat": "no-repeat, no-repeat, no-repeat",
            "background-size": "cover, cover, 100% 10rem",
            "background-position": "center, center, bottom"
        }
    },
    ".frmdb-t-cover .frmdb-t-main": {
        "addClasses": [
            "grid-row-1-3",
            "grid-align-self-center",
            "d-flex",
            "flex-column",
            "justify-content-center",
            "frmdb-section-dark"
        ]
    },
    ".frmdb-t-cover .frmdb-t-aside": {
        "addClasses": [
            "grid-row-3-4",
            "container",
            "border",
            "rounded-sm",
            "shadow-sm",
            "bg-secondary",
            "py-4"
        ]
    },
    ".frmdb-t-cover .frmdb-t-aside > *": {
        "addClasses": [
            "m-3"
        ]
    },
    ".frmdb-t-header": {
        "addClasses": [
            "min-vh-50",
            "overflow-hidden",
            "d-flex",
            "flex-column",
            "justify-content-center",
            "frmdb-bg-parallax",
            "frmdb-bg-2",
            "frmdb-bg--user--dark-solid-tint-50"
        ]
    },
    ".jumbotron": {
        "addClasses": [
            "w-100",
            "text-center",
            "bg-transparent",
            "border-0"
        ]
    },
    ".frmdb-t-card-deck": {
        "addClasses": [
            "card-deck"
        ]
    },
    ".frmdb-t-card-media-main": {
        "addClasses": [
            "frmdb-card-img-10",
            "card",
            "border-0",
            "d-grid",
            "frmdb-card-img-3"
        ]
    },
    ".frmdb-t-card-media-main .frmdb-t-img": {
        "addClasses": [
            "frmdb-bg-2",
            "frmdb-bg-2-dark-solid-tint-50",
            "grid-row-1-4",
            "h-100"
        ],
        "addStyles": {
            "background-repeat": "no-repeat, no-repeat",
            "background-size": "cover, cover",
            "background-position": "center, center"
        }
    },
    ".frmdb-t-card-media-main .card-body": {
        "addClasses": [
            "text-center",
            "grid-row-2-3"
        ]
    },
    ".frmdb-t-card-media-main .card-body *": {
        "addClasses": [
            "text-light"
        ]
    },
    ".frmdb-t-card-media-main .frmdb-t-card-action": {
        "addClasses": [
            "grid-row-3-4",
            "grid-align-self-end",
            "grid-justify-self-center",
            "mb-2"
        ]
    },
    ".frmdb-t-card-media-main .frmdb-t-card-note": {
        "addClasses": [
            "grid-row-1-2",
            "text-center",
            "text-info",
            "font-weight-bold"
        ]
    },
    ".frmdb-t-card-media-main .frmdb-t-card-action a": {
        "addClasses": [
            "btn",
            "btn-primary",
            "m-auto"
        ]
    },
    ".frmdb-t-media-section-main": {
        "addClasses": [
            "frmdb-section-dark",
            "py-5",
            "frmdb-bg-3",
            "frmdb-bg-2-divider-slope-rv",
            "frmdb-bg-3-divider-slope"
        ],
        "addStyles": {
            "background-repeat": "no-repeat, no-repeat, no-repeat",
            "background-size": "cover, 100% 10rem, 100% 10rem",
            "background-position": "center, top, bottom"
        }
    },
    ".frmdb-section-dark .frmdb-t-section-divider": {
        "addClasses": [
            "py-5"
        ]
    },
    ".frmdb-section-light .frmdb-t-section-divider": {
        "addClasses": [
            "py-4"
        ]
    },
    ".frmdb-t-section-cards-icon": {
        "addClasses": [
            "my-5",
            "py-5",
            "frmdb-section-dark",
            "frmdb-bg-4",
            "frmdb-bg-2-dark-solid-tint-75",
            "frmdb-bg-3-divider-slope-rv",
            "frmdb-bg-4-divider-slope"
        ],
        "addStyles": {
            "background-repeat": "no-repeat, no-repeat, no-repeat, no-repeat",
            "background-size": "cover, cover, 100% 10rem, 100% 10rem",
            "background-position": "center, center, top, bottom"
        }
    },
    ".frmdb-t-section-cards-icon .frmdb-t-section-divider": {
        "addClasses": [
            "py-5",
            "my-3"
        ]
    },
    ".frmdb-t-card-icon-main": {
        "addClasses": [
            "border-light",
            "p-4",
            "bg-transparent"
        ]
    },
    "body > section": {
        "addClasses": [
            "my-3"
        ]
    },
    "body > footer": {
        "addClasses": [
            "pt-4",
            "bg-dark",
            "frmdb-section-dark"
        ]
    }
}