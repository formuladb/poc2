{
    ".frmdb-t-main-nav": [
        "container",
        "bg-white",
        "fixed-top",
        "shadow"
    ],
    ".frmdb-t-main-nav .navbar": [
        "navbar-light",
        "navbar-expand-lg"
    ],
    ".frmdb-t-cover": [
        "min-vh-100",
        "overflow-hidden",
        "frmdb-section-dark"
    ],
    ".frmdb-t-cover .frmdb-t-background": [
        "grid-row-1-4",
        "grid-col-1-1",
        "-frmdb-bg-tint-secondary-50",
        "frmdb-bg-primary-circles-2"
    ],
    ".frmdb-t-cover .frmdb-t-main": [
        "grid-row-1-3",
        "grid-col-1-1",
        "grid-align-self-center",
        "d-flex",
        "flex-column",
        "justify-content-center"
    ],
    ".frmdb-t-cover .frmdb-t-aside": [
        "grid-row-3-4",
        "grid-col-1-1",
        "container",
        "frmdb-section-dark",
        "z-index-overlap"
    ],
    ".frmdb-t-cover .frmdb-t-aside > *": [
        "m-3"
    ],
    ".frmdb-t-section-divider": [
        "h-8-em",
        "bg-white",
        "skew-y-n4-down"
    ], 
    ".frmdb-t-header": [
        "min-vh-50",
        "overflow-hidden",
        "d-flex",
        "flex-column",
        "justify-content-center",
        "frmdb-bg-tint-secondary-50"
    ],    
    ".jumbotron": [
        "w-100",
        "text-center",
        "bg-transparent",
        "border-0"
    ],
    ".frmdb-t-card-media-main": [
        "border-0",
        "d-grid"
    ],
    ".frmdb-t-card-media-main .frmdb-t-img": [
        "grid-row-1-1",
        "grid-col-1-1"
    ],
    ".frmdb-t-card-media-main .frmdb-t-img img": [
        "w-100"
    ],
    ".frmdb-t-card-media-main .card-body": [
        "text-center"
    ],
    ".frmdb-t-card-media-main .frmdb-t-card-note": [
        "text-center",
        "text-info",
        "font-weight-bold"
    ],
    ".frmdb-t-card-media-main .frmdb-t-card-action": [
        "grid-row-1-1",
        "grid-col-1-1",
        "grid-align-self-end",
        "grid-justify-self-center",
        "mb-2"
    ],
    ".frmdb-t-card-media-main .frmdb-t-card-action a": [
        "btn",
        "btn-primary",
        "m-auto"
    ],
    ".frmdb-t-card-icon-main": [
        "border-light",
        "p-4",
        "bg-transparent"
    ]
}