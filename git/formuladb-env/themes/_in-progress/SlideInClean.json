{
    ".frmdb-t-main-nav": {
        "addClasses": [
            "container",
            "bg-white",
            "fixed-top",
            "shadow"
        ]
    },
    ".frmdb-t-main-nav .navbar": {
        "addClasses": [
            "navbar-light",
            "navbar-expand-lg"
        ]
    },
    ".frmdb-t-cover": {
        "addClasses": [
            "min-vh-100",
            "overflow-hidden",
            "d-flex",
            "flex-column",
            "justify-content-between",
            "frmdb-bg-tint-parallax",
            "frmdb-bg-tint-secondary-50"
        ],
        "addCssVars": {
            "--frmdb-bg-img": "url(\"/formuladb-env/static/frmdb-apps/base-app/bg1.jpg\")"
        }
    },
    ".frmdb-t-cover .frmdb-t-main": {
        "addClasses": [
            "flex-grow-1",
            "d-flex",
            "flex-column",
            "justify-content-center"
        ]
    },
    ".frmdb-t-cover .frmdb-t-aside": {
        "addClasses": [
            "container",
            "frmdb-section-dark"
        ]
    },
    ".frmdb-t-cover .frmdb-t-aside > *": {
        "addClasses": [
            "m-3"
        ]
    },
    ".jumbotron": {
        "addClasses": [
            "w-100",
            "text-center",
            "bg-transparent"
        ],
        "addAttributes": {
            "data-aos": "slideInUp"
        }
    },
    ".frmdb-t-card-media-main": {
        "addClasses": [
            "border-0"
        ],
        "addStyles": {
            "display": "grid"
        },
        "addAttributes": {
            "data-aos": "slideInLeft",
            "data-aos-once": "false"
        },
        ".frmdb-t-card-media-main .frmdb-t-img": {
            "addStyles": {
                "grid-row": "1 / 1",
                "grid-column": "1 / 1"
            }
        },
        ".frmdb-t-card-media-main .card-body": {
            "addClasses": [
                "text-center"
            ]
        },
        ".frmdb-t-card-media-main .frmdb-t-card-note": {
            "addClasses": [
                "text-center",
                "text-info",
                "font-weight-bold"
            ]
        },
        ".frmdb-t-card-media-main .frmdb-t-card-action": {
            "addStyles": {
                "grid-row": "1 / 1",
                "grid-column": "1 / 1",
                "grid-align-self": "end",
                "grid-justify-self": "center"
            },
            "addClasses": [
                "mb-2"
            ]
        },
        ".frmdb-t-card-media-main .frmdb-t-card-action a": {
            "addClasses": [
                "btn",
                "btn-primary",
                "m-auto"
            ]
        }
    },
    ".frmdb-t-card-icon-main": {
        "addClasses": [
            "border-light",
            "p-4",
            "bg-transparent"
        ]
    }
}