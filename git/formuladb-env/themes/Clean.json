{
    ".frmdb-t-main-nav": [
        "container",
        "bg-white",
        "fixed-top",
        "shadow"
    ],
    ".frmdb-t-main-nav .navbar": [
        "navbar-light",
        "navbar-expand-lg"
    ],
    ".frmdb-t-cover": [
        "min-vh-100",
        "overflow-hidden",
        "d-flex",
        "flex-column",
        "justify-content-between",
        "frmdb-bg-tint-parallax",
        "frmdb-bg-tint-secondary-50"
    ],
    ".frmdb-t-cover .frmdb-t-main": [
        "flex-grow-1",
        "d-flex",
        "flex-column",
        "justify-content-center"
    ],
    ".frmdb-t-cover .frmdb-t-aside": [
        "container",
        "frmdb-section-dark"
    ],
    ".frmdb-t-cover .frmdb-t-aside > *": [
        "m-3"
    ],
    ".frmdb-t-header": [
        "min-vh-50",
        "overflow-hidden",
        "d-flex",
        "flex-column",
        "justify-content-center",
        "frmdb-bg-tint-parallax",
        "frmdb-bg-tint-secondary-50"
    ],    
    ".jumbotron": [
        "w-100",
        "text-center",
        "bg-transparent",
        "border-0"
    ],
    ".frmdb-t-card-img": [
        "border-0",
        "d-grid"
    ],
    ".frmdb-t-card-img .frmdb-t-img": [
        "grid-row-1-1",
        "grid-col-1-1"
    ],
    ".frmdb-t-card-img .frmdb-t-img img": [
        "w-100"
    ],
    ".frmdb-t-card-img .card-body": [
        "text-center"
    ],
    ".frmdb-t-card-img .frmdb-t-card-note": [
        "text-center",
        "text-info",
        "font-weight-bold"
    ],
    ".frmdb-t-card-img .frmdb-t-card-action": [
        "grid-row-1-1",
        "grid-col-1-1",
        "grid-align-self-end",
        "grid-justify-self-center",
        "mb-2"
    ],
    ".frmdb-t-card-img .frmdb-t-card-action a": [
        "btn",
        "btn-primary",
        "m-auto"
    ],
    ".frmdb-t-card-icon": [
        "border-light",
        "p-4",
        "bg-transparent"
    ]
}