{
    ".frmdb-t-main-nav": [
        "container",
        "bg-white",
        "fixed-top",
        "shadow"
    ],
    ".frmdb-t-main-nav .navbar": [
        "navbar-light",
        "navbar-expand-lg"
    ],
    ".frmdb-t-cover": [
        "min-vh-100",
        "overflow-hidden",
        "frmdb-section-dark",
        "frmdb-bg-parallax",
        "frmdb-bg-tint-secondary-50"
    ],
    ".frmdb-t-cover .frmdb-t-main": [
        "grid-row-1-3",
        "grid-align-self-center",
        "d-flex",
        "flex-column",
        "justify-content-center"
    ],
    ".frmdb-t-cover .frmdb-t-aside": [
        "grid-row-3-4",
        "container",
        "frmdb-section-dark"
    ],
    ".frmdb-t-cover .frmdb-t-aside > *": [
        "m-3"
    ],
    ".frmdb-t-header": [
        "min-vh-50",
        "overflow-hidden",
        "d-flex",
        "flex-column",
        "justify-content-center",
        "frmdb-bg-tint-parallax",
        "frmdb-bg-tint-secondary-50"
    ],    
    ".jumbotron": [
        "w-100",
        "text-center",
        "bg-transparent",
        "border-0"
    ],
    ".frmdb-t-card-deck": [
        "card-deck"
    ],
    ".frmdb-t-card-media-main": [
        "card",
        "border-0",
        "d-grid",
        "frmdb-card-img-3"
    ],
    ".frmdb-t-card-media-main .frmdb-t-img": [
        "frmdb-bg",
        "grid-row-1-3"
    ],
    ".frmdb-t-card-media-main .card-body": [
        "text-center"
    ],
    ".frmdb-t-card-media-main .frmdb-t-card-action": [
        "grid-row-2-3",
        "grid-align-self-end",
        "grid-justify-self-center",
        "mb-2"
    ],
    ".frmdb-t-card-media-main .frmdb-t-card-note": [
        "text-center",
        "text-info",
        "font-weight-bold"
    ],
    ".frmdb-t-card-media-main .frmdb-t-card-action a": [
        "btn",
        "btn-primary",
        "m-auto"
    ],
    ".frmdb-t-media-section-main": [
        "frmdb-section-dark",
        "py-5",
        "frmdb-bg",
        "frmdb-bg-parallax"
    ],
    ".frmdb-t-card-icon-main": [
        "border-light",
        "p-4",
        "bg-transparent"
    ],
    "body > section": [
        "my-3"
    ],
    "body > footer": [
        "pt-4",
        "bg-dark",
        "frmdb-section-dark"
    ]
}