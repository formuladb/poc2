  "scripts": {
    "_util": "for i in */*.scss; do echo -n $i:${i/scss/min.css}' '; done",
    "generate:watch": "watch 'bash ci/build.sh' `echo */ | sed -e 's!/!!g; s/node_modules//; s/_scss//; s/_css//'`",
    "sass:watch": "sass --watch --style compressed frmdb _scss:_css",
    "watch": "tmux new-session 'npm run generate:watch' ';' split-window -h 'npm run sass:watch'",
    "generate": "bash ci/build.sh; sass _scss:_css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
