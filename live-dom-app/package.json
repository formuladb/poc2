{
  "name": "live-dom-app",
  "version": "1.0.0",
  "description": "live editing of the DOM on the current page",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "rimraf dist && concurrently --kill-others \"npm run build:watch\" \"npm run serve\"",
    "serve": "webpack-dev-server",
    "compile:watch": "tsc --sourceMap -w -p .",
    "build:watch": "webpack --watch --progress --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/jquery": "^3.3.29",
    "@types/node": "^12.0.2",
    "concurrently": "^4.1.0",
    "dom-testing-library": "^4.1.0",
    "jasmine": "^3.4.0",
    "jsdom": "^15.1.0",
    "jsdom-global": "^3.0.2",
    "live-server": "^1.2.1",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "sass": "^1.20.1",
    "terser-webpack-plugin": "^1.2.4",
    "testem": "^2.16.0",
    "ts-loader": "^6.0.1",
    "typescript": "3.1.6",
    "webpack": "^4.32.1",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "delegated-events": "^1.0.10",
    "jquery": "^3.4.1",
    "module-alias": "^2.2.0",
    "navigo": "^7.1.2",
    "source-map-support": "^0.5.12"
  }
}
